/* Simple closure test */
enum ListStr | Cons(String, ListStr) | Nil

func Map((String) -> String, ListStr) -> ListStr
rule Map(#, ())           → ()
rule Map(#, Cons(#1:String, #s:ListStr)) → Cons(#(#1), Map(#, #s))

func Head(ListStr) -> String
rule Head(()) → ""
rule Head(Cons(#1:String, #s:ListStr)) → #1

func Tests -> String
→ Head(Map((x : String) -> x : String, Cons("1", Cons("2", Nil))))
