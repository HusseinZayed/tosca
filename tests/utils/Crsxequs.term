(

/* **Constructor Equality** */

Assert[ TRUE,
        ConstructorEqual[ %constructor ⟦ ##qualifier B ⟧ ,%constructor ⟦ ##qualifier B ⟧]
      ];

Assert[ FALSE,
        ConstructorEqual[ %constructor ⟦ ##qualifier A ⟧ ,%constructor ⟦ ##qualifier B ⟧]
      ];

/* **Binder Equality** */

Assert[ TRUE,
        BinderEqual[%binder ⟦ ##annotations? x ⟧, %binder ⟦ ##annotations? x ⟧]
      ];

Assert[ FALSE,
        BinderEqual[%binder ⟦ ##annotations? x ⟧, %binder ⟦ ##annotations? y ⟧]
      ];

Assert[ FALSE,
        BinderEqual[%binder ⟦ ##annotations? xi ⟧, %binder ⟦ ##annotations? xj ⟧]
      ];

)